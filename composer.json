{
  "name": "26b/unbabble",
  "description": "A new and simple i18n system for WordPress",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://github.com/26B/unbabble",
  "license": "GPL-2.0+",
  "require": {
    "php": ">=7.4",
    "composer/installers": "^1.11",
    "dg/composer-cleaner": "^2.2",
    "ramsey/uuid": "^4.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.7"
  },
  "authors": [],
  "autoload": {
    "psr-4": {
      "TwentySixB\\WP\\Plugin\\Unbabble\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwentySixB\\WP\\Plugin\\Unbabble\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "dg/composer-cleaner": true
    }
  }
}
