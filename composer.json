{
  "name": "26b/unbabble",
  "description": "A new and simple i18n system for WordPress",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "https://github.com/26B/unbabble",
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
        "type": "vcs",
        "url":  "https://github.com/26B/diqa-formatter"
    }
  ],
  "require": {
    "php": ">=8.0",
    "diqa/formatter": "1.1",
    "ramsey/uuid": "^4.6",
    "wp-cli/wp-cli": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.7"
  },
  "authors": [],
  "autoload": {
    "psr-4": {
      "TwentySixB\\WP\\Plugin\\Unbabble\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwentySixB\\WP\\Plugin\\Unbabble\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": false
    }
  }
}
